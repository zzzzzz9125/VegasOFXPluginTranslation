        <OfxParamTypeInteger name="41">
          <OfxPropLabel>상세도</OfxPropLabel>
          <OfxParamPropHint>웨이브의 노이즈 옥타브 수입니다.</OfxParamPropHint>
        </OfxParamTypeInteger>
        <OfxParamTypeBoolean name="25">
          <OfxPropLabel>색도 수직 블렌드</OfxPropLabel>
